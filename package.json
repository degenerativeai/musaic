{
  "name": "musaic-dataset-architect",
  "author": "degenerativeai",
  "private": false,
  "version": "1.1.1",
  "type": "module",
  "description": "A high-fidelity prompt engineering suite designed to generate, refine, and structure realistic datasets for AI training and evaluation.",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite --port 5175 --strictPort",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none npm run dev\" \"wait-on http://localhost:5175 && \\\"node_modules/electron/dist/electron.exe\\\" .\"",
    "electron:build": "vite build && electron-builder"
  },
  "build": {
    "appId": "com.musaic.app",
    "productName": "Musaic",
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "release"
    },
    "win": {
      "target": "nsis",
      "icon": "electron/icon.png"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "dependencies": {
    "@google/genai": "latest",
    "@types/file-saver": "^2.0.7",
    "electron-squirrel-startup": "^1.0.1",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.5",
    "electron-builder": "^26.0.12",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^7.2.4",
    "wait-on": "^9.0.3"
  }
}